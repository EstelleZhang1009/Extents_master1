#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_make-plural@3.0.6@make-plural/bin/make-plural" "$@"
  ret=$?
else 
  node  "$basedir/../../../_make-plural@3.0.6@make-plural/bin/make-plural" "$@"
  ret=$?
fi
exit $ret
